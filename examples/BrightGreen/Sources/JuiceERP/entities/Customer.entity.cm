entity:
   id: Customer
   name: "Customer"
   attributes:
   - id: CustomerID
     name: "CustomerID"
     datatype: "Integer"  
     identifier: true
   - id: FirstName
     name: "FirstName"
     datatype: "Text"
   - id: LastName
     name: "LastName"
     datatype: "Text"
   - id: DateOfBirth
     name: "DateOfBirth"
     datatype: "Date"
   - id: EmailAddress
     name: "EmailAddress"
     datatype: "Text"