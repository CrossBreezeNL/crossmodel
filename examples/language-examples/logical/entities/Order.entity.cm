entity:
    id: Order
    name: "Order"
    description: "Order placed by a customer in the Customer table."
    attributes:
      - id: Order_Id
        name: "Order Id"
        datatype: "Integer"
      - id: OrderDate
        name: "OrderDate"
        datatype: "DateTime"
        precision: 2
      - id: OrderNumber
        name: "OrderNumber"
        datatype: "Text"
        length: 50
      - id: Customer_Id
        name: "Customer Id"
        datatype: "Integer"
      - id: TotalAmount
        name: "TotalAmount"
        datatype: "Decimal"
        precision: 38
        scale: 6
    identifiers:
      - id: Order_Surrogate_Identifier
        name: "Order Surrogate Identifier"
        attributes:
          - Order_Id