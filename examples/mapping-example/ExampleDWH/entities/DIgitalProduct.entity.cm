entity:
    id: DigitalProduct
    name: "DigitalProduct"
    inherits:
      - Product
    attributes:
      - id: Attribute
        name: "ProductID"
        datatype: "Guid"
        identifier: true
      - id: SizeInKB
        name: "SizeInKB"
        datatype: "Decimal"
      - id: DownloadURL
        name: "DownloadURL"
        datatype: "Text"